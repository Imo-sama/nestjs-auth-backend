generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model User {
  id                 String   @id @default(cuid())
  email              String   @unique
  password           String
  twoFactorSecret    String?
  twoFactorEnabled   Boolean  @default(false)
  createdAt          DateTime @default(now())
  updatedAt          DateTime @updatedAt
}
